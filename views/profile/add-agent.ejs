<div class="modal-dialog modal-m">
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title">Add Agent</h5>
            <button type="button" class="close" data-dismiss="modal">&times;</button>
        </div>
        <div class="modal-body">
            <div class="alert alert-warning alert-dismissible fade show" id="alertEmpty" role="alert" style="display: none;">
                <strong>Warning!</strong> Please fill in all input below.
                <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                  <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <form class="" action="#">
                <div class="form-group row mb-2">
                    <label class="col-form-label col-sm-3">Name<span class="text-danger"></span></label>
                    <div class="col-lg-9">
                        <input class="searchInput" id="i_agent_name" type="text" name="name">
                    </div>
                </div>
                <div class="form-group row mb-2">
                    <label class="col-form-label col-sm-3">Email<span class="text-danger"></span></label>
                    <div class="col-lg-9">
                        <input class="searchInput" id="i_agent_email" type="text" name="email">
                    </div>
                </div>
                <div class="form-group row mb-2">
                    <label class="col-form-label col-sm-3">Phone No.<span class="text-danger"></span></label>
                    <div class="col-lg-9">
                        <input class="searchInput" id="i_agent_phone" type="text" name="phone">
                    </div>
                </div>
                <div class="form-group row mb-2">
                    <label class="col-form-label col-sm-3">Address<span class="text-danger"></span></label>
                    <div class="col-lg-9">
                        <input class="searchInput" id="i_agent_address" type="text" name="address">
                    </div>
                </div>
            </form>
           
        </div>
        <div class="modal-footer">
            <button class="btn" data-dismiss="modal">
                <i class="font-size-base mr-1"></i> Cancel
            </button>
            <button class="btn btn-success ml-1" id="btn_add_agent_to_database">Add</button>
        </div>
    </div>
</div>