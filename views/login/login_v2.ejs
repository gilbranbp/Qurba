<!DOCTYPE html>
<html lang="en">
  <head>
    <title>login</title>

    <!-- Global stylesheets -->
    <link href="/global_assets/css/icons/icomoon/styles.min.css" rel="stylesheet" type="text/css">
    <link href="/assets/css/all.min.css" rel="stylesheet" type="text/css">
    <!-- /global stylesheets -->

     <!-- Core JS files -->
     <script src="/global_assets/js/main/jquery.min.js"></script>
     <!-- /core JS files -->

</head>

<body>

    <div class="page-content login-cover">

		<!-- Main content -->
		<div class="content-wrapper">

			<!-- Content area -->
			<div class="content d-flex justify-content-center align-items-center">

           <div class="row" style="margin-top:-50px;">
              <div class="col-md-12">

                  <form class="login-form">
                      <div class="card" >
                          <div class="card-body">
                                  <div class="text-center mb-3">
                                      <img src="/global_assets/images/logotsel.png" class="rounded-circle mr-2 pt-0 border-slate-300" height="60" alt=""/>
                                      <h5 class="mb-0">Welcome to ICM Dashboard</h5>
                                      <span class="d-block text-muted">Login to your account</span>
                                  </div>

                                  <div class="form-group form-group-feedback form-group-feedback-left">
                                      <input id="inp" type="text" placeholder="username" title="Please enter you username" required="" value="" name="username" class="form-control">
                                      <div class="form-control-feedback">
                                          <i class="icon-user text-muted"></i>
                                      </div>
                                  </div>

                                  <div class="form-group form-group-feedback form-group-feedback-left">
                                      <input type="password" class="form-control example1" name="password" placeholder="Password" value="">
                                      <span class="help-block small" id="err" style="display:none">Err</span>
                                      <div class="form-control-feedback">
                                          <i class="icon-lock2 text-muted"></i>
                                      </div>
                                  </div>

                                  

                                  <div class="form-group">
                                      <button  class="btn btn-primary btn-block" id="login">Sign in <i class="icon-circle-right2 ml-2"></i></button>
                                  </div>

                                  <div class="form-group text-center text-muted content-divider">
                                      <span class="px-2">Sign in application</span>
                                      <ul id="events"></ul>
                                  </div>

                                  <!-- <div class="row" style="margin-top: -30px; height: 20px;">
                                      <div class="col-md-11">
                                          
                                      </div>
                                      <div class="col-md-1">
                                          <a href="/download" ><i class="icon-bookmark mt-1 icon-2x ml-1 icon-1x text-primary"></i></a>
                                          
                                      </div>
                                  </div> -->
                              
                          </div>
                      </div>
                  </form>

              </div>
          </div>
			</div>
			<!-- /content area -->

		</div>
		<!-- /main content -->

    </div>
   
    <script src="scr/login/login.js" type="text/javascript"></script>
    <!-- <script src="/global_assets/js/plugins/socket/socket.io.js"></script> -->

</body>
</html>